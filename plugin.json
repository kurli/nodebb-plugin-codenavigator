{
    "id": "nodebb-plugin-codenavigator",
    "name": "NodeBB Plugin CodeNavigator",
    "description": "NodeBB plugin allows users to clone git repo via CodeNavigator on iOS",
    "url": "https://github.com/kurli/nodebb-plugin-codenavigator",
    "library": "./index.js",
    "hooks": [
        { "hook": "filter:parse.post", "method": "parse", "priority": 4 },
        { "hook": "filter:parse.post", "method": "parse", "priority": 6 }
    ],
    "staticDirs": {
        "styles": "./static"
    },
    "less": [
        "./static/styles/style.less"
    ],
    "scripts": [
        "static/lib/main.js"
    ]
}
